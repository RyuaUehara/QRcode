generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Helper {
  id        Int      @id @default(autoincrement())
  helperID  String   @unique
  helperName String?
}

model Customer {
  id          Int      @id @default(autoincrement())
  customerName String?
}

model AttendanceRecord {
  id        Int      @id @default(autoincrement())
  helperID  String
  helperName String
  customerName String
  checkIn   DateTime?
  checkOut  DateTime?
}